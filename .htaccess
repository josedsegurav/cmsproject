# Turn on the rewriting engine
RewriteEngine on
# Handle index page
RewriteRule ^./$ index.php [NC,L]
# Handle create items request
RewriteRule ^items/add$ create.php [NC,L]
# Handle item edit request
RewriteRule ^items/edit/([0-9]+)/?$ edit.php?id=$1 [NC,L]
# Handle process item request
RewriteRule ^items/process?$ process.php [NC,L]